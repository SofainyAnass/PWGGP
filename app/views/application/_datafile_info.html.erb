
<tr>
		<td><%= @datafile.nom %></td>
		<td><%= @datafile.versions.first.nom %></td>
		<td><%= @datafile.versions.first.user.contact.nom_complet %></td>
		<td><%= @datafile.updated_at %></td>
		<td><%= link_to pluralize(@datafile.versions.count , "version"), "#{versions_path}?id=#{@datafile.id}" %></td>
		<td><%= link_to "Télécharger", download_version_path(@datafile.versions.first) %></td>
		<td><%= link_to "Supprimer", @datafile, :method => :delete %></td>
</tr>