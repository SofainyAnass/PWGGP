<!DOCTYPE html>
<html>
<head>
	
  <title> <%= @titre %> </title>
  
  <meta name="viewport" content="width=device-width, maximum-scale=1">
  
   <%= csrf_meta_tags %>
  
   <link rel="icon" href="favicon.png" type="image/png">	
    
    
    <%= stylesheet_link_tag 'sidebar.css' %>
    	
	<%= stylesheet_link_tag 'bootstrap.min.css' %>
	<%= stylesheet_link_tag 'style.css' %>
	<%= stylesheet_link_tag 'font-awesome.css' %>
	<%= stylesheet_link_tag 'animate.css' %>	
	
	
	<%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>	
		
	<%= javascript_include_tag 'application' %>	

 	<!--[if lt IE 9]>
	    <script src="js/respond-1.1.0.min.js"></script>
	    <script src="js/html5shiv.js"></script>
	    <script src="js/html5element.js"></script>
	<![endif]-->
 
</head>
<body>

<% if signed_in? %>
	
<%= render 'layouts/header' %> 

<%= render 'layouts/side_menu' %>

<% end %>

<%= yield %>


<%= render 'layouts/footer' %> 



<%= debug(params) if Rails.env.development? %>

	

	<%= javascript_include_tag 'jquery-1.11.0.min.js' %>	
	<%= javascript_include_tag 'bootstrap.min.js' %>
	<%= javascript_include_tag 'jquery-scrolltofixed.js' %>
	<%= javascript_include_tag 'jquery.nav.js' %>	
	<%= javascript_include_tag 'jquery.easing.1.3.js' %>
	<%= javascript_include_tag 'jquery.isotope.js' %>	
	<%= javascript_include_tag 'wow.js' %>
	<%= javascript_include_tag 'custom.js' %>
	
	<%= javascript_include_tag 'jquery.ssd-vertical-navigation.min.js' %>
	<%= javascript_include_tag 'sidebar.js' %>	
	

</body>
</html>